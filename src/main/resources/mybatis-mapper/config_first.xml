    <typeAlias alias="roleModel" type="com.shinvest.ap.app.role.model.RoleModel"/>
    <typeAlias alias="roleGroupModel" type="com.shinvest.ap.app.role.model.RoleGroupModel"/>
    <typeAlias alias="scheduleLogModel" type="com.shinvest.ap.app.schedule.model.ScheduleLogModel"/>
    <typeAlias alias="scheduleModel" type="com.shinvest.ap.app.schedule.model.ScheduleModel"/>
    <typeAlias alias="wrkModel" type="com.shinvest.ap.app.wrkCat.model.WrkModel"/>
    <typeAlias alias="aprvModel" type="com.shinvest.ap.app.aprv.model.AprvModel"/>
    
