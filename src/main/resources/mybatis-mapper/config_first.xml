  <typeAliases>
    <typeAlias alias="map" type="java.util.Map"/>
    <typeAlias alias="faqModel" type="com.shinvest.ap.app.board.model.FaqModel"/>
    <typeAlias alias="noticeModel" type="com.shinvest.ap.app.board.model.NoticeModel"/>
    <typeAlias alias="codeModel" type="com.shinvest.ap.app.code.model.CodeModel"/>
    <typeAlias alias="deptClModel" type="com.shinvest.ap.app.dept.model.DeptClModel"/>
    <typeAlias alias="extrnlModel" type="com.shinvest.ap.app.extrnl.model.ExtrnlModel"/>
    <typeAlias alias="fileModel" type="com.shinvest.ap.app.file.model.FileModel"/>
    <typeAlias alias="licenseModel" type="com.shinvest.ap.app.license.model.LicenseModel"/>
    <typeAlias alias="logModel" type="com.shinvest.ap.app.audit.model.LogModel"/>
    <typeAlias alias="memberCriteria" type="com.shinvest.ap.app.member.model.MemberCriteria"/>
    <typeAlias alias="memberModel" type="com.shinvest.ap.app.member.model.MemberModel"/>
    <typeAlias alias="menuModel" type="com.shinvest.ap.app.menu.model.MenuModel"/>
    <typeAlias alias="projectCriteria" type="com.shinvest.ap.app.project.model.ProjectCriteria"/>
    <typeAlias alias="projectModel" type="com.shinvest.ap.app.project.model.ProjectModel"/>
    <typeAlias alias="reportAuthModel" type="com.shinvest.ap.app.report.model.ReportAuthModel"/>
